  "lyrics": [
    {
      "time": 2.38,
      "text": "Is this your photo album, Fran?"
    },
    {
      "time": 6.46,
      "text": "Yes! These are pictures of my family."
    },
    {
      "time": 10.8,
      "text": "Who are they?"
    },
    {
      "time": 13.96,
      "text": "That's my father and mother."
    },
    {
      "time": 16.85,
      "text": "How many brothers do you have?"
    },
    {
      "time": 20.46,
      "text": "I have three brothers -- Stan, Dan, and Van."
    },
    {
      "time": 26.49,
      "text": "And who are they?"
    },
    {
      "time": 30,
      "text": "They're my sisters -- Jan and Nan."
    },
    {
      "time": 34.11,
      "text": "Who's she?"
    },
    {
      "time": 37.47,
      "text": "She's my baby sister."
    },
    {
      "time": 40.02,
      "text": "What's her name?"
    },
    {
      "time": 42.93,
      "text": "Ann."
    }
  ]
